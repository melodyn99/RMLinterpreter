include "send_back.rmx"

/* Your implementation of [send_back] should be such that the following code
 * outputs ["first"] before ["second"]. */

let rec fib n =
  if n = 1 then 1
  else if n = 2 then 1
  else (fib (n-1)) + (fib (n-1))

in
let _ = send_back (fun x -> ignore(fib x); println "second") 23 in
println "first";
sleep 1000000
