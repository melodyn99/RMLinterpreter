include "map_async.rmx"

/* Your implementation of [map_async] should be such that the following code
 * outputs ["first"] and then some indeterminate order of ["one"], ["two"],
 * and ["three"]. */

let rec fib n =
   if n = 1 then 1
   else if n = 2 then 1
   else (fib (n-1)) + (fib (n-1))
in

let println' x =
  ignore begin
  fib 23 end; println x
in

let _ = map_async println' ["one";"two";"three"] in
println "first";
sleep 10000000
